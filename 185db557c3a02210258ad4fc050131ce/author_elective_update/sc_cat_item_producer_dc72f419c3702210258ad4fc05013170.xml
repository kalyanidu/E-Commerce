        <script><![CDATA[// Create a Request
var request = new GlideRecord('sc_request');
request.initialize();
request.requested_for = gs.getUserID(); // current user
request.requested_by = gs.getUserID();
var reqID = request.insert();

// Create Requested Item (RITM)
var ritm = new GlideRecord('sc_req_item');
ritm.initialize();
ritm.request = reqID;
ritm.requested_for = gs.getUserID();
ritm.cat_item = current.getValue('sys_id'); // record producer ID
ritm.short_description = "E-Commerce Product Order";
var ritmID = ritm.insert();

// Create a new record in your custom E-commerce Order table
var order = new GlideRecord('x_1585559_ecommerc_order_items'); // use full table name here
order.initialize();
order.product_name = current.variables.product_name;
order.total_price=current.variables.total_price;

order.u_request = request.sys_id; // link the request if you have this field
order.insert();
]]></script>
